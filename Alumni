<script type="text/javascript" >
  function checkEmpty()
  {

	if(document.frmMemberReg.DBSPname.value=="")
	{
		window.alert("Please Enter Name");
		document.frmMemberReg.DBSPname.focus();
		return false;
	}
		if(document.frmMemberReg.DBSPdob.value=="")
	{
		window.alert("Please Enter Date Of Birth");
		document.frmMemberReg.DBSPdob.focus();
		return false;
	}
		if(document.frmMemberReg.DBSPgender.value=="")
	{
		window.alert("Please Enter Gender");
		document.frmMemberReg.DBSPgender.focus();
		return false;
	}
		if(document.frmMemberReg.DBSPaddress.value=="")
	{
		window.alert("Please Enter Address");
		document.frmMemberReg.DBSPaddress.focus();
		return false;
	}
		if(document.frmMemberReg.DBSPcity.value=="")
	{
		window.alert("Please Enter City");
		document.frmMemberReg.DBSPcity.focus();
		return false;
	}
		if(document.frmMemberReg.DBSPpin.value=="")
	{
		window.alert("Please Enter Pin");
		document.frmMemberReg.DBSPpin.focus();
		return false;
	}
		if(document.frmMemberReg.DBSPstate.value=="")
	{
		window.alert("Please Enter State");
		document.frmMemberReg.DBSPstate.focus();
		return false;
	}
		if(document.frmMemberReg.DBSPCountry.value=="")
	{
		window.alert("Please Enter Name");
		document.frmMemberReg.DBSPCountry.focus();
		return false;
	}
		if(document.frmMemberReg.DBSPmob.value=="")
	{
		window.alert("Please Enter Mobile No");
		document.frmMemberReg.DBSPmob.focus();
		return false;
	}
		if((document.frmMemberReg.DBSPemail.value=="")||(document.frmMemberReg.DBSPemail.value.indexOf('@') == -1)||(document.frmMemberReg.DBSPemail.value.indexOf('.') == -1))
	{

		window.alert("Please Enter Valid Email Id");
		document.frmMemberReg.DBSPemail.focus();
		return false;
	}
  }
</script>
